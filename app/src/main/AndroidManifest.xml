    package="com.example.petersam.teroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FleksiActivity" />
        <activity android:name=".EkstensiActivity" />
        <activity android:name=".PronasiActivity" />
        <activity android:name=".SupinasiActivity" />
        <activity android:name=".DeviasiUlnarActivity" />
        <activity android:name=".DeviasiRadialActivity"/>
    </application>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

</manifest>